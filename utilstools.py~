def gen(a,b):
    if a > b:
        a += 1
        while a != b:
            a -= 1
            yield a
    if a < b:        
        a -= 1
        while a != b:
            a += 1
            yield a        

def replaceWhiteSpace(string):
    string = ('  ' + string + '  ')
    return string
